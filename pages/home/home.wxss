.home-contain{  
  height: 128rpx;
  background-color: #5ac54d;
  position: fixed;
  top: 0;
  z-index: 3;
  width: 100%;
}
.nav-tabs {
  width: 100%;
  height: 75rpx;
  display: flex;
  /* margin: 128rpx 0 0 0; */
  position: fixed;
  top: 50rpx;
  z-index: 2;
} 
.tab-content {
  flex: 1;
}
.active {
  width: 100rpx;
  line-height: 80rpx;
  text-align: center;
  color: #0091ff;
  border-bottom: 2px solid #0091ff;
  font-size: 28rpx;
}
.default{
  width: 100rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}